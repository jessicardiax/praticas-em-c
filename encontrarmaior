

#include<stdio.h>
void no_return(float a, float b, float c){
    float maior;
    
    if (a>=b && a>=c){
        maior = a;
    }
    else if (b>=a && b>=c){
        maior = b;
    }
    else {
        maior = c;
    }
    
    printf("O valor maior Ã© %.2f\n", maior);
    
}
 int main(){
     float x,y,z;
    
    printf("Digite 3 valores reais:\n");
    scanf("%f %f %f", &x,&y,&z);
    
    no_return(x,y,z);
    
    return 0;
 }
