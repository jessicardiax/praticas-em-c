

#include<stdio.h>
 int main(){
     int A[8];
     int i;
     
      printf("Informe 8 valores inteiros\n");
     for(i=0; i<8; i++){
       printf("Valor %d", i + 1);
           scanf("%d",&A[i]);
 }
 
    printf("Valore na ordem inversa\n");
    for(i=7; i>=0; i--){
        printf("%d", A[i]);
    }
    
    printf("\n");
    
return 0;
}
